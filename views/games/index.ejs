<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Games List</title>
</head>
<body>
    <header>
        <ul>
            <li><a href="/games/new">Add Game</a></li>
        </ul>
    </header>
    <h1>All Games</h1>
    <ul>
        <% gameDocs.forEach(game => { %>
         <li>
            <a href="/games/<%= game._id %>">
                <%= game.name %></li>
            </a>
        <% }) %>
    </ul>
</body>
</html>